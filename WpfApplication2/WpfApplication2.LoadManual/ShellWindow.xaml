<Window x:Class="WpfApplication2.LoadManual.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication2.LoadManual"
        xmlns:prism="http://prismlibrary.com/"
        mc:Ignorable="d"
        Title="ShellWindow" Height="300" Width="300">
    <DockPanel LastChildFill="True">
        <Button DockPanel.Dock="Top" Content="Load Module" Click="Button_Click" />
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
    </DockPanel>
</Window>
